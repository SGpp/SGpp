{
	"dataSource": {
		"filePath": "../datasets/DR5/DR5_train.arff",
		"validationPortion": 0.5,
		"hasTargets": false,
		"readinCutoff": 4000,
		"readinColumns": [0, 1, 2]
	},
	"scorer": {
		"metric": "mse"
	},
	"fitter": {
		"type": "regressionLeastSquares",
		"gridConfig": {
			"gridType": "linear",
			"level": 3
		},
		"adaptivityConfig": {
			"numRefinements": 5,
			"threshold": 0.001,
			"noPoints": 10,
			"refinementIndicator": "surplus",
			"errorBasedRefinement": false,
			"refinementPeriod": 500
		},
		"solverRefineConfig": {
			"solverType": "cg",
			"eps": 10e-15,
			"maxIterations": 100,
			"threshold": 1
		},
		"solverFinalConfig": {
			"solverType": "cg",
			"eps": 10e-15,
			"maxIterations": 100,
			"threshold": 1
		},
		"regularizationConfig": {
			"lambda": 10e-7
		}
	}
}
