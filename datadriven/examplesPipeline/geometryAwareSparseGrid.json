{
  	"dataSource": {
    		"filePath": "../datasets/dataread/simple.arff",
		"batchSize": 10
	},
	"fitter": {

    		"gridConfig": {
      			"gridType": "modlinear",
      			"level": "3"
    		},
		"geometryConfig": {
			"dim":[
				3,
				2
			],
			"stencil": "DN"
		},
    		"regularizationConfig":{
			"regularizationType": "Identity",
      			"lambda": "0.01"
    		},
    		"densityEstimationConfig":{
			"densityEstimationType": "decomposition",
			"matrixDecompositionType": "Chol"
    		},
    		"adaptivityConfig":{
      			"numRefinements": "10",
      			"noPoints": "5",
      			"threshold": "0.0"
    		},
		"learner" : {
			"usePrior" : false,
			"beta" : 0.0
		}

  	}
}
