SRCDIR = ./../../../src/application_cpp/
BUILDDIR = ./
BINDIR = ./../../../bin/
LIBDIR = ./../../../lib/sgpp/
OBJS = NativeCppClassifyBenchmark.o

default: $(OBJS)
ifeq ($(CC),icl)
	$(CC) $(LFLAGS) $(BUILDDIR)NativeCppClassifyBenchmark.obj $(LIBDIR)$(LIBNAME) /Fe$(BINDIR)$(BINNAME)
	cp $(BINDIR)$(BINNAME) $(BINDIR)ClassifyBenchmark_${EXT}.exe
else
	$(CC) $(BUILDDIR)NativeCppClassifyBenchmark.o $(LIBDIR)$(LIBNAME) $(LFLAGS) -o $(BINDIR)$(BINNAME)
	cp $(BINDIR)$(BINNAME) $(BINDIR)ClassifyBenchmark_$(EXT)
endif
	
NativeCppClassifyBenchmark.o : $(SRCDIR)NativeCppClassifyBenchmark.cpp
	$(CC) $(CFLAGS) -o $(BUILDDIR)NativeCppClassifyBenchmark.o $(SRCDIR)NativeCppClassifyBenchmark.cpp
ifeq ($(CC),icl)
	cp $(BUILDDIR)NativeCppClassifyBenchmark.obj $(BUILDDIR)NativeCppClassifyBenchmark.o
endif
	
clean:
	rm *
