WINOBJS=$(OBJS:.o=.obj)

ifeq ($(CC),g++)
NOIPO=
default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME).a
	rm -f $(BINDIR)$(LIBNAME).so
	ar rc $(BINDIR)$(LIBNAME).a $(OBJS)
	ranlib $(BINDIR)$(LIBNAME).a
	$(CC) -shared -fopenmp -o $(BINDIR)$(LIBNAME).so $(OBJS)
endif

ifeq ($(CC),mpigxx)
NOIPO=
default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME).a
	rm -f $(BINDIR)$(LIBNAME).so
	ar rc $(BINDIR)$(LIBNAME).a $(OBJS)
	ranlib $(BINDIR)$(LIBNAME).a
	$(CC) -shared -fopenmp -o $(BINDIR)$(LIBNAME).so $(OBJS)
endif

ifeq ($(CC),CC)
NOIPO=
default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME).a
	rm -f $(BINDIR)$(LIBNAME).so
	ar rc $(BINDIR)$(LIBNAME).a $(OBJS)
	ranlib $(BINDIR)$(LIBNAME).a
	$(CC) -shared -fopenmp -o $(BINDIR)$(LIBNAME).so $(OBJS)
endif

ifeq ($(CC),icpc)
NOIPO=-no-ipo -no-ip
default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME).a
	rm -f $(BINDIR)$(LIBNAME).so
	rm -f $(BINDIR)$(LIBNAME)MIC.a
	xiar $(XIAR_OPTS) rc $(BINDIR)$(LIBNAME).a $(OBJS)
	ranlib $(BINDIR)$(LIBNAME).a
	$(CC) $(LIB_OPTS) -shared -openmp -o $(BINDIR)$(LIBNAME).so $(OBJS)
endif

ifeq ($(CC),mpiicpc)
NOIPO=-no-ipo -no-ip
default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME).a
	rm -f $(BINDIR)$(LIBNAME).so
	rm -f $(BINDIR)$(LIBNAME)MIC.a
	xiar $(XIAR_OPTS) rc $(BINDIR)$(LIBNAME).a $(OBJS)
	ranlib $(BINDIR)$(LIBNAME).a
	$(CC) $(LIB_OPTS) -shared -openmp -o $(BINDIR)$(LIBNAME).so $(OBJS)
endif

ifeq ($(CC),mpiCC)
NOIPO=-no-ipo -no-ip
default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME).a
	rm -f $(BINDIR)$(LIBNAME).so
	rm -f $(BINDIR)$(LIBNAME)MIC.a
	xiar rc $(BINDIR)$(LIBNAME).a $(OBJS)
	ranlib $(BINDIR)$(LIBNAME).a
	$(CC) -shared -openmp -o $(BINDIR)$(LIBNAME).so $(OBJS)
endif

ifeq ($(CC),icl)
NOIPO=
default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME).lib
	xilib /OUT:$(BINDIR)$(LIBNAME).lib $(WINOBJS)
endif

