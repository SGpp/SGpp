ifeq ($(EXT), OCL)
OBJS:=$(OBJS) OperationMultipleEvalIterativeOCLLinear.o\
	OperationMultipleEvalIterativeSPOCLLinear.o\
	OperationMultipleEvalIterativeSPHybridX86SimdOCLLinear.o\
	OperationMultipleEvalIterativeHybridX86SimdOCLLinear.o\
	OCLKernels.o\
	OperationMultipleEvalIterativeOCLModLinear.o\
	OperationMultipleEvalIterativeSPOCLModLinear.o\
	OperationMultipleEvalIterativeSPHybridX86SimdOCLModLinear.o\
	OperationMultipleEvalIterativeHybridX86SimdOCLModLinear.o
OCLCFLAGS=-Wall -ansi -c -fno-strict-aliasing -O3 -funroll-loops -I$(SRCDIR) -I$(OCLINCLUDE) -DUSEOCL -DUSEOCL_NVIDIA
OCLCC=g++
endif

ifeq ($(EXT), IOCL)
OBJS:=$(OBJS) OperationMultipleEvalIterativeOCLLinear.o\
 	OperationMultipleEvalIterativeSPOCLLinear.o\
	OperationMultipleEvalIterativeSPHybridX86SimdOCLLinear.o\
	OperationMultipleEvalIterativeHybridX86SimdOCLLinear.o\
	OCLKernels.o\
	OperationMultipleEvalIterativeOCLModLinear.o\
	OperationMultipleEvalIterativeSPOCLModLinear.o\
	OperationMultipleEvalIterativeSPHybridX86SimdOCLModLinear.o\
	OperationMultipleEvalIterativeHybridX86SimdOCLModLinear.o
