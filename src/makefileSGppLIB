ifeq ($(EXT), ArBB)
OBJS:=$(OBJS) OperationMultipleEvalIterativeArBBLinear.o OperationMultipleEvalIterativeSPArBBLinear.o ArBBKernels.o
endif
ifeq ($(EXT), OCL)
OBJS:=$(OBJS) OperationMultipleEvalIterativeOCLLinear.o OperationMultipleEvalIterativeSPOCLLinear.o\
	OperationMultipleEvalIterativeSPHybridSSEOCLLinear.o OperationMultipleEvalIterativeHybridSSEOCLLinear.o OCLKernels.o
OCLCFLAGS=-Wall -ansi -c -fno-strict-aliasing -O3 -funroll-loops -I$(SRCDIR) -I$(OCLINCLUDE) -DUSEOCL
OCLCC=g++
endif
ifeq ($(EXT), IOCL)
OBJS:=$(OBJS) OperationMultipleEvalIterativeOCLLinear.o OperationMultipleEvalIterativeSPOCLLinear.o\
	OperationMultipleEvalIterativeSPHybridSSEOCLLinear.o OperationMultipleEvalIterativeHybridSSEOCLLinear.o OCLKernels.o
OCLCFLAGS=-Wall -ansi -c -fno-strict-aliasing -O3 -funroll-loops -I$(SRCDIR) -I$(IOCLINCLUDE) -DUSEOCL -DUSEOCL_CPU
OCLCC=g++
endif
ifeq ($(EXT), MPI)
OBJS:=$(OBJS) MPICommunicator.o GlobalMPIComm.o ConjugateGradientsMPI.o PoissonEquationSolverMPI.o\
	PoissonEquationEllipticPDESolverSystemDirichletParallelMPI.o HeatEquationParabolicPDESolverSystemParallelMPI.o\
	HeatEquationSolverMPI.o BlackScholesSolverMPI.o BlackScholesParabolicPDESolverSystemEuropeanParallelMPI.o\
	BiCGStabMPI.o
endif

default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME)
ifeq ($(CC),icpc)
	xiar rc $(BINDIR)$(LIBNAME) $(OBJS)
endif
ifeq ($(CC),mpiicpc)
	xiar rc $(BINDIR)$(LIBNAME) $(OBJS)
endif
ifeq ($(CC),g++)
	ar rc $(BINDIR)$(LIBNAME) $(OBJS)
endif
	ranlib $(BINDIR)$(LIBNAME)

