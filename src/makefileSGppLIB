endif

default : $(OBJS)
	mkdir -p $(BINDIR)
	rm -f $(BINDIR)$(LIBNAME)
ifeq ($(CC),icpc)
	xiar rc $(BINDIR)$(LIBNAME) $(OBJS)
endif
ifeq ($(CC),mpiicpc)
	xiar rc $(BINDIR)$(LIBNAME) $(OBJS)
endif
ifeq ($(CC),g++)
	ar rc $(BINDIR)$(LIBNAME) $(OBJS)
endif
	ranlib $(BINDIR)$(LIBNAME)

