CFLAGS=-Wall -ansi -c -fno-strict-aliasing -fno-alias -ansi-alias -O3 -funroll-loops -I$(SRCDIR) -I$(ARBBINCLUDES) -openmp -DUSEOMP -DUSEOMPTHREE -DUSEICCINTRINSICS -DUSEARBB -axSSE3
LFLAGS=-Wall -ansi -O3 -openmp -L$(ARBBLIBS) -larbb -ltbb

